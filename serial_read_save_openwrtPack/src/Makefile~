objects=main.o
#generate final exe file using the exsiting -o or -a lib file
serial_read_save:$(objects)
	$(CC) $(LDFLAGS) -o serial_read_save $(objects)
#generata -o file ,it is lib file 
$(objects):main.c data.h dir.h serial.h protocol.h
	$(CC) $(CFLAGS) -c main.c -std=gnu99

#clean operation
.PHONY:clean
clean:
	-rm serial_read_save *.o
